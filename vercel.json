{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps --force",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
    "NPM_CONFIG_FORCE": "true"
  }
}
